# Example production environment variables
# Copy this to .env.production and fill in your actual values
# DO NOT commit .env.production to Git!

# Vercel Configuration
VERCEL_TOKEN=your-vercel-token-from-vercel.com/account/tokens
VERCEL_ORG_ID=your-organization-id-from-vercel-settings
VERCEL_PROJECT_ID_API=your-api-project-id-from-vercel-project-settings
VERCEL_PROJECT_ID_WEB=your-web-project-id-from-vercel-project-settings

# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database

# Security Configuration
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters-long
ENCRYPTION_KEY=your-super-secret-encryption-key-exactly-32-chars

# API Configuration (optional - for custom domains)
API_URL=https://your-custom-api-domain.com

# GitHub OAuth (optional)
GITHUB_CLIENT_ID=your-github-oauth-client-id
GITHUB_CLIENT_SECRET=your-github-oauth-client-secret

# GitHub App Integration (optional)
GITHUB_APP_ID=your-github-app-id
GITHUB_APP_NAME=your-github-app-name
GITHUB_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
your-private-key-content-here
-----END RSA PRIVATE KEY-----"