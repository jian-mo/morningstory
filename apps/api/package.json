{
  "name": "@morning-story/api",
  "version": "0.1.0",
  "description": "Morning Story API",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "dev:simple": "ts-node src/main-simple.ts",
    "dev:full": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "nest build",
    "start": "node ../../dist/apps/api/src/main",
    "start:prod": "node dist/apps/api/src/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./jest.config.js --testMatch='**/*.e2e-spec.ts'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^5.1.0",
    "@octokit/auth-app": "^7.2.1",
    "@octokit/rest": "^19.0.13",
    "@prisma/client": "^5.8.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/passport-github2": "^1.2.0",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.0",
    "@types/supertest": "^6.0.0",
    "jest": "^29.7.0",
    "prisma": "^5.8.0",
    "source-map-support": "^0.5.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0"
  }
}
